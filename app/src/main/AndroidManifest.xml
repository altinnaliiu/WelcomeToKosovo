<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.welcometokosova">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Welcome To Kosovo"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.WelcomeToKosova">
        <meta-data
            android:name="com.google.android.geo.API_kEY"
            android:value="AIzaSyCDDW2FuiFe4JAUT5x6C1hypJvQ4iLTHt8"></meta-data>

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Informata"
            android:parentActivityName=".MainActivity"
            />
        <activity android:name=".Hiking"
            android:parentActivityName=".MainActivity"
            />
        <activity android:name=".Culture"
            android:parentActivityName=".MainActivity"
            />
        <activity android:name=".Feedback"
            android:parentActivityName=".MainActivity"
            />
    </application>

</manifest>